{
    "name": "hello-world",
    "displayName": "hello-world",
    "description": "",
    "version": "0.0.1",
    "publisher": "liuderchi",
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.markdownInitTemplate",
        "onCommand:extension.sayHello"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sayHello",
                "title": "Hello World: Activate"
            },
            {
                "command": "extension.wordCount",
                "title": "Hello World: Word Count"
            },
            {
                "command": "extension.markdownInitTemplate",
                "title": "Markdown: Insert Init Template"
            },
            {
                "command": "extension.greet",
                "title": "Hello World: Greet"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
