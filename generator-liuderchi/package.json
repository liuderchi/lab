{
  "name": "generator-liuderchi",
  "version": "0.0.0",
  "description": "generate boilerplate for liuderchi",
  "homepage": "",
  "author": {
    "name": "liuderchi",
    "email": "liuderchi@gmail.com",
    "url": ""
  },
  "files": [
    "generators"
